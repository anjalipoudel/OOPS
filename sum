#include<iostream>
#include<conio.h>
using namespace std;
class substract 
{
   float z;
public:

   substract() 
   {
       cout << "difference  is:";
   }

   substract(float a, float b) 
   {
       z = a + b;
   }

   void display() 
   {
       cout << z;
   }
};

int main()
 {

   float a, b;

   cout << "Enter two numbers:";
   cin >> a>>b;

   substract t;
   substract tt(a, b);
   tt.display();
   getch();
   return 0;
}
